pipeline{
agent any
      stages{
            stage("Build"){
              steps{
                   https://github.com/devsecops2020/JavaHelloWorld.git 
              stage("Compile Stage"){
              steps{
                  withMaven(jdk:'JDK')
                  withMaven(maven:'maven-3.6.3'){
                  bat "mvn clean compile'}
                   }
                                 }
            stage("Testing Stage"){
              steps{
                  withMaven(jdk:'JDK')
                  withMaven(maven:'maven-3.6.3'){
                  bat 'mvn maven test'}
                   }
                                 }
            stage("Deployment Stage"){
              steps{
                  withMaven(jdk:'JDK')
                  withMaven(maven:'maven-3.6.3'){
                  bat 'mvn maven deploy'}
                  }
                                    }
             }
         }
